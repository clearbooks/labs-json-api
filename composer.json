        "clearbooks/dilex": "^4.0.0-alpha",
        "clearbooks/dilex-jwt": "^4.0.0-alpha",
        "clearbooks/labs": "^2.2.0",
        "clearbooks/labs-mysql": "^4.0.0",
        "php-di/php-di": "^7.1.1"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^9.6.9"
    },
    "license": "LGPL-v3",
    "authors": [
