{
    "name": "clearbooks/labs-json-api",
    "description": "The labs JSON api",
    "type": "project",
    "require": {
        "clearbooks/labs": "1.0.0",
        "zircote/swagger-php": "dev-master",
        "clearbooks/labs-mysql": "1.0.0",
        "clearbooks/dilex-jwt": "^1.0",
        "php-di/php-di": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "tomverran/di-container-mock": "*"
    },
    "license": "LGPL-v3",
    "authors": [
        {
            "name": "Crimson Ducks",
            "email": "support@clearbooks.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/clearbooks/labs"
        },
        {
            "type": "vcs",
