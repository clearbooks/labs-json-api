{
    "name": "clearbooks/labs-json-api",
    "description": "The labs JSON api",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "zircote/swagger-php": "^3.1.0",
        "clearbooks/dilex": "^3.0.0-alpha.1",
        "clearbooks/dilex-jwt": "^3.0.0-alpha.1",
        "clearbooks/labs": "^2.0.0",
        "clearbooks/labs-mysql": "^3.0.0",
        "php-di/php-di": "^6.3.0"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^8.5.13"
    },
    "license": "LGPL-v3",
    "authors": [
        {
            "name": "Crimson Ducks",
            "email": "support@clearbooks.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/clearbooks/labs"
        },
        {
            "type": "vcs",
            "url": "https://github.com/clearbooks/labs-mysql"
        },
        {
            "type": "vcs",
            "url": "https://github.com/clearbooks/labs-php-client"
        },
        {
            "type": "vcs",
            "url": "https://github.com/clearbooks/labs-php-client-mysql"
        },
        {
            "type": "vcs",
            "url": "https://github.com/clearbooks/labs-schema"
        },
        {
            "type": "vcs",
            "url": "https://github.com/clearbooks/dilex"
        },
        {
            "type": "vcs",
            "url": "https://github.com/clearbooks/dilex-jwt"
        },
        {
            "type": "vcs",
            "url": "https://github.com/tomverran/di-container-mock"
        }
    ],
    "autoload": {
        "psr-4": {
            "Clearbooks\\LabsApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clearbooks\\LabsApi\\": "tests/"
        }
    },
    "minimum-stability": "stable"
